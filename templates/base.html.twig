<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header class="apside-header">
            <div class="search border border-1 d-flex mt-5">
                <input type="search-input" class="form-control border-0" id="search" name="search">
                <img class="loupe" src="{{ asset('build/images/loupe.png') }}">
            </div>
            <img class="apside-header-logo" src="{{ asset('build/images/logos/Apside_collab.png') }}">
        </header>
        {% block body %}
        {% endblock %}
    </body>
</html>
